# https://EditorConfig.org
root = true

[*.{dart,json,md,plist,yaml,yml}]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.dart]
indent_style = space
indent_size = 2
max_line_length = 120

[*.plist]
indent_style = space
indent_size = 4
max_line_length = 120
